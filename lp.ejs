                    <script>

                        function myFunction(id) {
                            const element = document.getElementById(id);

                            // Get the current position of the element relative to the document
                            const elementPosition = element.getBoundingClientRect().top + window.pageYOffset;

                            // Scroll to that position with an offset
                            window.scrollTo({
                                top: elementPosition - 14,  // Adjust the -100 to move it further down or up
                                behavior: 'smooth'  // Smooth scrolling
                            });
                        }



                        // Initialize the Swiper
                        const swiper = new Swiper('.mylpSwiper', {
                            loop: true,
                            autoplay: {
                                delay: 2000,
                                behavior: 'smooth'
                            }, speed: 1000,
                        });

                        // Event listener for slide change
                        swiper.on('slideChange', function () {
                            const activeSlide = swiper.slides[swiper.activeIndex];
                            const title = activeSlide.getAttribute('data-title');
                            const subtitle = activeSlide.getAttribute('data-subtitle');

                            // Update the content dynamically
                            document.getElementById('banner-title').textContent = title;
                            document.getElementById('banner-subtitle').textContent = subtitle;
                        });



                        document.addEventListener("DOMContentLoaded", function () {
                            // Hero Swiper
                            const heroSwiper = new Swiper(".heroSwiper", {
                                slidesPerView: 1,
                                loop: true,
                                autoplay: {
                                    delay: 5000,
                                    disableOnInteraction: false,
                                },
                                speed: 1000,
                            });


                            // General Swiper
                            const mySwiper = new Swiper(".mySwiper-p", {
                                slidesPerView: 1,
                                spaceBetween: 20,
                                loop: true,
                                pagination: {
                                    el: ".swiper-pagination",
                                    clickable: true,
                                },
                                breakpoints: {
                                    640: { slidesPerView: 1 },
                                    768: { slidesPerView: 2 },
                                    1024: { slidesPerView: 3 },
                                }
                            });

                            // Fancybox (Ensure Fancybox script is included)
                            if (typeof Fancybox !== "undefined") {
                                Fancybox.bind('[data-fancybox="gallery"]', {});
                            }


                            // Responsive Navbar
                            const navMenu = document.getElementById("nav-menu");
                            const menuIcon = document.getElementById("menu-icon");

                            navMenu.classList.remove("open");

                            menuIcon.addEventListener("click", function () {
                                navMenu.classList.toggle("open");
                            });


                            // Counting Animation
                            let counters = document.querySelectorAll(".box h2");
                            let section = document.querySelector(".service-provider");
                            let hasAnimated = false;

                            function animateCount(start, end, element, duration) {
                                let increment = (end - start) / (duration / 10);
                                let current = start;
                                let interval = setInterval(() => {
                                    current += increment;
                                    if (current >= end) {
                                        clearInterval(interval);
                                        element.textContent = end;
                                    } else {
                                        element.textContent = Math.floor(current);
                                    }
                                }, 10);
                            }

                            let observer = new IntersectionObserver((entries) => {
                                if (entries[0].isIntersecting && !hasAnimated) {
                                    hasAnimated = true;
                                    counters.forEach(counter => {
                                        let endValue = parseInt(counter.getAttribute("data-count"));
                                        animateCount(0, endValue, counter, 1000);
                                    });
                                }
                            }, { threshold: 0.5 });

                            if (section) {
                                observer.observe(section);
                            }

                            // Scroll Button
                            const scrollButton = document.querySelector(".scroll-button");

                            function scrolltopFunction() {
                                window.addEventListener("scroll", () => {
                                    if (window.scrollY > 200) {
                                        scrollButton.style.display = "block";
                                    } else {
                                        scrollButton.style.display = "none";
                                    }
                                });

                                scrollButton.addEventListener("click", () => {
                                    window.scrollTo({ top: 0, behavior: "smooth" });
                                });
                            }

                            scrolltopFunction();

                            // Modal JavaScript
                            const dialog = document.getElementById("dialog-p");
                            if (dialog) {
                                const openButtons = document.querySelectorAll(".show-dialog");
                                const closeBtn = dialog.querySelector(".close-btn-p");

                                openButtons.forEach((button) => {
                                    button.addEventListener("click", () => {
                                        dialog.showModal();
                                    });
                                });

                                closeBtn.addEventListener("click", (e) => {
                                    e.preventDefault();
                                    dialog.close();
                                });
                            }
                        });


                        //testominals-swiper
                        const testimonialSwiper = new Swiper('.testimonial-swiper', {
                            direction: 'horizontal',
                            loop: true,
                            pagination: {
